{
    "title": "Home Row Mods in Dvorak",
    "rules": [
        {
            "description": "a: command, o: shift, e: option, u: control",
            "manipulators": [
                {
                    "type": "basic",
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                { "is_built_in_keyboard": true }
                            ]
                        }
                    ],
                    "from": {
                        "key_code": "a",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "parameters": {
                        "basic.to_if_alone_timeout_milliseconds": 200,
                        "basic.to_if_held_down_threshold_milliseconds": 200
                    },
                    "to_if_alone": {
                        "halt": true,
                        "key_code": "a"
                    },
                    "to_delayed_action": {
                        "to_if_canceled": [
                            {
                                "key_code": "a"
                            }
                        ]
                    },
                    "to_if_held_down": {
                        "halt": true,
                        "key_code": "left_command"
                    }
                },
